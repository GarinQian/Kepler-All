<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<alias name="kepler.service.quiet" alias="kepler.service.imported.quiet" />
	<alias name="kepler.service.quiet" alias="kepler.service.exported.quiet" />

	<bean id="kepler.service.quiet" class="com.kepler.service.imported.QuietExceptions">
		<constructor-arg ref="kepler.methods" />
	</bean>

</beans>